{
  "manifest_version": 2,

  "name": "Strava feed filter",
  "description": "Hide virtual rides, commutes and other cruft from Activity Feed.",
  "version": "2.4.0",

  "permissions": [
    "storage"
  ],

  "icons": { "128": "strava2-f1.128px.png" },

  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "content_scripts": [ {
    "js": [
      "jquery-3.1.1.min.js",
      "app.js"
    ],
    "matches": [
      "*://www.strava.com/dashboard",
      "*://www.strava.com/dashboard?*",
      "*://www.strava.com/dashboard/*"
    ]
  }]
}
